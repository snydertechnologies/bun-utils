    "@jscutlery/semver": "^3.1.0",
    "@nx/esbuild": "16.10.0",
    "@nx/eslint-plugin": "16.10.0",
    "@nx/jest": "16.10.0",
    "@nx/js": "16.10.0",
    "@nx/linter": "16.10.0",
    "@nx/plugin": "16.10.0",
    "@nx/vite": "16.10.0",
    "@nx/workspace": "16.10.0",
    "@swc-node/register": "1.6.8",
    "@swc/cli": "~0.1.62",
    "@swc/core": "1.3.93",
    "@types/fs-extra": "^11.0.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.5",
    "@types/kill-port": "^2.0.1",
    "@types/node": "20.6.3",
    "@types/semver": "^7.5.2",
