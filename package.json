    "fs-extra": "^11.1.1",
    "nx": "16.10.0",
    "semver": "7.5.4",
    "strong-log-transformer": "^2.1.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@angular-devkit/core": "^16.2.3",
